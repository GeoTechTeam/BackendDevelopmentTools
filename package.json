{
  "name": "backenddevelopingtools",
  "version": "1.0.4",
  "description": "A startup template of node express postgres stack for further development on alt linux distributive",
  "main": "app.js",
  "scripts": {
    "start": "node app.js",
    "server": "nodemon app.js",
    "client": "npm run dev --prefix ../client",
    "build": "npm run build --prefix ../client",
    "buildpwa": "npm run buildpwa --prefix ../client",
    "dev": "concurrently \"npm run server\" \"npm run client\""
  },
  "author": "GeoTechTeam",
  "license": "MIT",
  "dependencies": {
    "activedirectory": "^0.7.2",
    "activedirectory2": "^2.1.0",
    "bcrypt": "^5.1.1",
    "concurrently": "^7.6.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "exceljs": "^4.3.0",
    "express": "^4.18.2",
    "fast-xml-parser": "^4.1.3",
    "fparser": "^2.0.2",
    "kerberos": "^2.0.1",
    "multer": "^1.4.5-lts.1",
    "nodemon": "^2.0.20",
    "pg": "^8.8.0",
    "pg-promise": "^11.5.2",
    "pm2": "^5.3.0",
    "winston": "^3.12.0"
  }
}
